<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="LibVirtConnectTest" class="org.libvirt.Connect">
	<constructor-arg value="test:///default" type="String"></constructor-arg>
	<constructor-arg value="false" type="boolean"></constructor-arg>
	</bean>

	<bean id="LibVirtComputeControllerTest" class="sddc.genericapi.libvirt.LibVirtComputeController">
	<constructor-arg><ref bean="LibVirtConnectTest" /></constructor-arg>
	</bean>
	
	<bean id="LibVirtNetworkControllerTest" class="sddc.genericapi.libvirt.LibVirtNetworkController">
	<constructor-arg><ref bean="LibVirtConnectTest" /></constructor-arg>
	</bean>
	
	<bean id="LibVirtStorageControllerTest" class="sddc.genericapi.libvirt.LibVirtStorageController">
	<constructor-arg><ref bean="LibVirtConnectTest" /></constructor-arg>
	</bean>
	
	<bean id="LibVirtServiceModuleHandlerTest" class="sddc.genericapi.ServiceModuleHandler">
	<constructor-arg>
		<list>
			<ref bean="LibVirtComputeControllerTest" />
			<ref bean="LibVirtNetworkControllerTest" />
			<ref bean="LibVirtStorageControllerTest" />
		</list>
	</constructor-arg>
	</bean>
	
	<bean id="WorkflowTest" class="sddc.services.domain.Workflow">
		<constructor-arg><ref bean="LibVirtServiceModuleHandlerTest" /></constructor-arg>
	</bean>
	
	
	             
</beans>