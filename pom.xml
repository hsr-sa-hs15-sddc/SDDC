    <modelVersion>4.0.0</modelVersion>
	<groupId>SDDC</groupId>
  <artifactId>SDDC</artifactId>
    <version>0.0.3-SNAPSHOT</version>
  	<name>SDDC</name>
	<url>http://sddc.silvn.com</url>
	<description>Software Defined Data Center</description>
	<organization>
		<name>HSR</name>
		<url>http://hsr.ch</url>
